{
    "version": "2.0.0",
    "tasks": [
        {
            "type": "process",
            "label": "build",
            "command": "C:/MinGW/bin/g++.exe",
            "args": [
                "-g",
                "-std=c++17",
                "main.cpp",
                "ArbolCuentas.cpp",
                "BackupManager.cpp",
                "Cliente.cpp",
                "Cuenta.cpp",
                "EstadisticasBancarias.cpp",
                "Fecha.cpp",
                "GestorClientes.cpp",
                "GestorIntegrado.cpp",
                "ListaCircularMenu.cpp",
                "MenuBancario.cpp",
                "NodoCuenta.cpp",
                "Transaccion.cpp",
                "Usuario.cpp",
                "-o",
                "${workspaceFolder}/main.exe"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}